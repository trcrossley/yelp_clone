<% if @restaurants.any? %>
  <% @restaurants.each do |restaurant| %>
    <h2> <%= link_to restaurant.name, restaurant_path(restaurant) %> </h2>
    <h2> <%= link_to "Review #{restaurant.name}", new_restaurant_review_path(restaurant) %> </h2>
    <h5> <%= link_to "Edit #{restaurant.name}", edit_restaurant_path(restaurant) %> </h5>
    <h5> <%= link_to "Delete #{restaurant.name}", restaurant_path(restaurant), method: :delete %> </h5>
  <% end %>
<% else %>
  No restaurants yet!
<% end %>



<a href='/restaurants/new'> Add a restaurant </a>
